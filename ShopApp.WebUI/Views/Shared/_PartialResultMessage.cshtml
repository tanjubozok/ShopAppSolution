@model ResultMessage

<div class="row">
    <div class="col-md-12">
        <div class="alert alert-@Model.Css alert-dismissible fade show" role="alert">
            <h4 class="alert-title">@Model.Title</h4>
            <p>@Model.Message</p>
            <button type="button" class="close" data-dismiss="alert" aria-label="Close">
                <span aria-hidden="true">&times;</span>
            </button>
        </div>
    </div>
</div>

